{
    "name": "weismannweb/hostingpanels",
    "description": "Laravel wrapper around OAuth 1 & OAuth 2 libraries.",
    "keywords": ["hosting control panels", "laravel", "cpanel", "plesk"],
    "license": "MIT",
    "homepage": "https://noneyet.com",
    "support": {
        "issues": "https://github.com/weismannweb/hostingpanels/issues",
        "source": "https://github.com/weismannweb/hostingpanels"
    },
    "authors": [
        {
            "name": "Henry Weismann",
            "email": "henry@weismannweb.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\HostingPanels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weismannweb\\HostingPanels\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "weismannweb": {
            "providers": [
                "Weismannweb\\HostingPanels\\HostingPanelsServiceProvider"
            ],
            "aliases": {
                "HostingPanels": "Weismannweb\\HostingPanels\\Facades\\HostingPanels"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable":  false
}
